{
  "name": "expo-db-sync",
  "version": "0.1.0",
  "bin": "bin/expo-db-sync.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:build": "tsc -p lambda/tsconfig.json && cp lambda/package.json lambda/dist/ || true && (cd lambda/dist && npm install --omit=dev)",
    "cdk:deploy": "yarn cdk:build && cdk deploy --require-approval never"
  },
  "devDependencies": {
    "@types/jest": "^30",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1100.3",
    "jest": "^30",
    "ts-jest": "^29",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.232.2",
    "constructs": "^10.0.0"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "lambda/*"
    ]
  }
}
